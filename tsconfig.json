{
  "include": [
    "packages",
    "types",
    "scripts",
    ".storybook/preview.tsx",
    "stories",
    "test",
    "./setupTests.ts"
  ],
  "exclude": ["node_modules", "**/dist/"],
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2018",
    "lib": ["dom", "esnext"],
    "sourceMap": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "jsx": "react",
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "noFallthroughCasesInSwitch": true,
    "allowJs": true,
    "typeRoots": ["./node_modules/@types", "./types"],
    "types": ["jest", "node"],
    "baseUrl": "./",
    "paths": {
      "@mui-treasury/theme-treasury": ["packages/theme-treasury/src/"],
      "@mui-treasury/component-sticker": ["packages/component-sticker/src"],
      "@mui-treasury/component-formaddon": ["packages/component-formaddon/src"],
      "@mui-treasury/component-groupy": ["packages/component-groupy/src"],
      "@mui-treasury/component-numberinput": [
        "packages/component-numberinput/src/"
      ],
      "@mui-treasury/component-numberspinner": [
        "packages/component-numberspinner/src/"
      ],
      "@mui-treasury/component-sociallink": [
        "packages/component-sociallink/src/"
      ],
      "@mui-treasury/component-sociallink/*": [
        "packages/component-sociallink/src/*"
      ],
      "@mui-treasury/component-emailsubscribe": [
        "packages/component-emailsubscribe/src/"
      ],
      "@mui-treasury/component-emailsubscribe/*": [
        "packages/component-emailsubscribe/src/*"
      ],
      "@mui-treasury/component-navigation": [
        "packages/component-navigation/src/"
      ],
      "@mui-treasury/component-navigation/*": [
        "packages/component-navigation/src/*"
      ],
      "@mui-treasury/component-pininput": ["packages/component-pininput/src/"],
      "@mui-treasury/component-pininput/*": [
        "packages/component-pininput/src/*"
      ],
      "@mui-treasury/component-birthdatefield": [
        "packages/component-birthdatefield/src/"
      ],
      "@mui-treasury/component-birthdatefield/*": [
        "packages/component-birthdatefield/src/*"
      ],
      "@mui-treasury/component-daymonthyear": [
        "packages/component-daymonthyear/src/"
      ],
      "@mui-treasury/component-daymonthyear/*": [
        "packages/component-daymonthyear/src/*"
      ],
      "@mui-treasury/component-creditcard": [
        "packages/component-creditcard/src/"
      ],
      "@mui-treasury/component-creditcard/*": [
        "packages/component-creditcard/src/*"
      ],
      "@mui-treasury/component-info": ["packages/component-info/src/"],
      "@mui-treasury/component-info/*": ["packages/component-info/src/*"],
      // hygen-component-target

      "@mui-treasury/style-sticker-glow": ["packages/style-sticker-glow/src/"],
      "@mui-treasury/style-button-bootstrap": [
        "packages/style-button-bootstrap/src/"
      ],
      "@mui-treasury/style-menu-bootstrap": [
        "packages/style-menu-bootstrap/src/"
      ],
      "@mui-treasury/style-textfield-ant": [
        "packages/style-textfield-ant/src/"
      ],
      "@mui-treasury/style-textfield-bootstrap": [
        "packages/style-textfield-bootstrap/src/"
      ],
      "@mui-treasury/style-textfield-standard": [
        "packages/style-textfield-standard/src/"
      ],
      "@mui-treasury/style-textfield-tailwind": [
        "packages/style-textfield-tailwind/src/"
      ],
      "@mui-treasury/style-slider-apollo": [
        "packages/style-slider-apollo/src/"
      ],
      "@mui-treasury/style-sociallink-ball": [
        "packages/style-sociallink-ball/src/"
      ],
      "@mui-treasury/style-sociallink-moon": [
        "packages/style-sociallink-moon/src/"
      ],
      "@mui-treasury/style-sociallink-poof": [
        "packages/style-sociallink-poof/src/"
      ],
      "@mui-treasury/style-sociallink-round": [
        "packages/style-sociallink-round/src/"
      ],
      "@mui-treasury/style-sociallink-zoom": [
        "packages/style-sociallink-zoom/src/"
      ],
      "@mui-treasury/style-emailsubscribe-bootstrap": [
        "packages/style-emailsubscribe-bootstrap/src/"
      ],
      "@mui-treasury/style-emailsubscribe-info": [
        "packages/style-emailsubscribe-info/src/"
      ],
      "@mui-treasury/style-emailsubscribe-minimal": [
        "packages/style-emailsubscribe-minimal/src/"
      ],
      "@mui-treasury/style-emailsubscribe-ready": [
        "packages/style-emailsubscribe-ready/src/"
      ],
      "@mui-treasury/style-emailsubscribe-translucent": [
        "packages/style-emailsubscribe-translucent/src/"
      ],
      "@mui-treasury/style-navigation-bulb": [
        "packages/style-navigation-bulb/src/"
      ],
      "@mui-treasury/style-navigation-float": [
        "packages/style-navigation-float/src/"
      ],
      "@mui-treasury/style-navigation-line": [
        "packages/style-navigation-line/src/"
      ],
      "@mui-treasury/style-navigation-point": [
        "packages/style-navigation-point/src/"
      ],
      "@mui-treasury/style-navigation-zoom": [
        "packages/style-navigation-zoom/src/"
      ],
      "@mui-treasury/style-navigation-plain": [
        "packages/style-navigation-plain/src/"
      ],
      "@mui-treasury/style-info-n01": ["packages/style-info-n01/src/"],
      "@mui-treasury/style-info-blog": ["packages/style-info-blog/src/"],
      "@mui-treasury/style-info-n02": ["packages/style-info-n02/src/"],
      "@mui-treasury/style-info-n03": ["packages/style-info-n03/src/"],
      // hygen-style-target

      "@mui-treasury/use-number-input": ["packages/use-number-input/src/"],
      "@mui-treasury/use-birthdate-input": [
        "packages/use-birthdate-input/src/"
      ],
      "@mui-treasury/use-daymonthyear-input": [
        "packages/use-daymonthyear-input/src/"
      ],
      "@mui-treasury/use-input-siblings": ["packages/use-input-siblings/src/"],
      "@mui-treasury/use-pin-input": ["packages/use-pin-input/src/"],
      "@mui-treasury/use-card-input": ["packages/use-card-input/src/"],
      "@mui-treasury/use-expiry-input": ["packages/use-expiry-input/src/"],
      // hygen-hook-target
      "@mui-treasury/layout": ["packages/layout/src"],
      "@mui-treasury/mockup/*": ["packages/mockup/src/*"],

      "@mui-treasury/types": ["packages/types/index.d.ts"]
    }
  }
}
